<!-- VolLockTray.csproj -->
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <EnableWindowsTargeting>true</EnableWindowsTargeting>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Platforms>x64</Platforms>
    <PlatformTarget>x64</PlatformTarget>

    <!-- Point to your icon file. Put the .ico next to the .csproj or adjust the path. -->
    <ApplicationIcon>Papirus-Team-Papirus-Devices-Audio-speakers.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <!-- Ensure the file is included in the project (build-time only; not copied at runtime). -->
    <Content Include="Papirus-Team-Papirus-Devices-Audio-speakers.ico" />
  </ItemGroup>
</Project>
